{
  "uid": "minimalworker",
  "title": "MinimalWorker Observability",
  "tags": ["minimalworker", "background-workers"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Worker Executions (Total)",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(minimalworker_worker_executions_total)",
          "legendFormat": "Total Executions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 100, "color": "orange"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Worker Errors (Total)",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(minimalworker_worker_errors_total) or vector(0)",
          "legendFormat": "Total Errors",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "orange"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Average Worker Duration",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "avg(rate(minimalworker_worker_duration_milliseconds_sum[5m]) / rate(minimalworker_worker_duration_milliseconds_count[5m]))",
          "legendFormat": "Avg Duration (ms)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Active Workers",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(minimalworker_worker_active)",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Worker Execution Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "rate(minimalworker_worker_executions_total[1m])",
          "legendFormat": "{{worker_name}} ({{worker_type}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 5,
      "title": "Worker Duration (p50, p95, p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(minimalworker_worker_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(minimalworker_worker_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(minimalworker_worker_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 6,
      "title": "Worker Executions by Name",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum by (worker_name) (minimalworker_worker_executions_total)",
          "legendFormat": "{{worker_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 9,
      "title": "Consecutive Failures by Worker",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 14},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "minimalworker_worker_consecutive_failures",
          "legendFormat": "{{worker_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 3, "color": "orange"},
              {"value": 5, "color": "red"}
            ]
          },
          "max": 10
        }
      }
    },
    {
      "id": 7,
      "title": "Worker Errors by Type",
      "type": "table",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 14},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "minimalworker_worker_errors_total",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {"Time": true, "__name__": true, "job": true, "instance": true},
            "indexByName": {},
            "renameByName": {
              "worker_name": "Worker",
              "worker_type": "Type",
              "exception_type": "Exception",
              "Value": "Count"
            }
          }
        }
      ]
    },
    {
      "id": 13,
      "title": "Last Success Time",
      "type": "stat",
      "gridPos": {"h": 6, "w": 24, "x": 0, "y": 22},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "minimalworker_worker_last_success_time_seconds * 1000",
          "legendFormat": "{{worker_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "dateTimeFromNow",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "id": 10,
      "title": "Worker Logs",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 28},
      "collapsed": false
    },
    {
      "id": 11,
      "title": "All Worker Logs",
      "type": "logs",
      "gridPos": {"h": 10, "w": 12, "x": 0, "y": 29},
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{service_name=\"MinimalWorker.Sample\"} | json",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    },
    {
      "id": 12,
      "title": "Error Logs Only",
      "type": "logs",
      "gridPos": {"h": 10, "w": 12, "x": 12, "y": 29},
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{service_name=\"MinimalWorker.Sample\"} |~ \"(?i)(error|fail|exception)\"",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    }
  ],
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "editable": true,
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  }
}
